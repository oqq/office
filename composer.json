        {
            "name": "Eric Braun",
            "email": "eb@oqq.be"
        }
    ],
    "require": {
        "php": "8.0.*",
        "ext-json": "*",
        "dompdf/dompdf": "^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "psr/http-message": "^1.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "infection/infection": "^0.21.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.1",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Oqq\\Office\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oqq\\Office\\Test\\": "tests/unit"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
